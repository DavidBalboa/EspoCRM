{
    "fields": {
        "name": {
            "type": "personName",
            "isPersonalData": true,
            "required": true,
            "tooltipText": "Ingresar Nombre",
            "tooltip": true
        },
        "salutationName": {
            "type": "enum",
            "options": [
                "",
                "Mr.",
                "Ms.",
                "Mrs.",
                "Dr."
            ]
        },
        "firstName": {
            "type": "varchar",
            "maxLength": 100,
            "default": "",
            "trim": true,
            "required": true,
            "tooltipText": "Ingrese Nombre",
            "isPersonalData": false,
            "audited": false,
            "readOnly": false,
            "tooltip": true
        },
        "lastName": {
            "type": "varchar",
            "maxLength": 100,
            "required": true,
            "default": "",
            "trim": true
        },
        "description": {
            "type": "text"
        },
        "emailAddress": {
            "type": "email",
            "isPersonalData": true
        },
        "phoneNumber": {
            "type": "phone",
            "typeList": [
                "Mobile",
                "Office"
            ],
            "defaultType": "Celular",
            "isPersonalData": true,
            "notStorable": true,
            "required": false,
            "tooltipText": "Ingresé Número de contacto  Eje celular: 954408013 - Oficina: 225185872",
            "tooltip": true
        },
        "address": {
            "type": "address",
            "isPersonalData": true
        },
        "addressStreet": {
            "type": "text",
            "maxLength": 255,
            "dbType": "varchar"
        },
        "addressCity": {
            "type": "varchar",
            "trim": true
        },
        "addressState": {
            "type": "varchar",
            "trim": true
        },
        "addressCountry": {
            "type": "varchar",
            "trim": true
        },
        "addressPostalCode": {
            "type": "varchar",
            "trim": true
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "rut": {
            "type": "varchar",
            "required": true,
            "trim": true,
            "maxLength": 20,
            "tooltipText": "Ingrese Rut  (Eje: 12.345.678-9)",
            "isPersonalData": false,
            "audited": false,
            "readOnly": false,
            "tooltip": true,
            "isCustom": true
        },
        "beneficioSolicitado": {
            "type": "enum",
            "required": true,
            "tooltipText": "Ingrese Beneficio",
            "isSorted": true,
            "isPersonalData": false,
            "options": [
                "Ninguno",
                "Educación",
                "Gimnasio"
            ],
            "audited": false,
            "readOnly": false,
            "tooltip": true,
            "isCustom": true,
            "default": "Ninguno"
        },
        "fechaSolicitud": {
            "notNull": false,
            "type": "datetime",
            "required": true,
            "default": "javascript: return this.dateTime.getNow(1);",
            "tooltipText": "Ingrese Fecha Solicitud",
            "isPersonalData": false,
            "useNumericFormat": false,
            "audited": false,
            "readOnly": false,
            "tooltip": true,
            "isCustom": true
        },
        "fechaDesde": {
            "notNull": false,
            "type": "date",
            "required": true,
            "useNumericFormat": false,
            "default": "javascript: return this.dateTime.getToday();",
            "isPersonalData": false,
            "audited": false,
            "readOnly": false,
            "tooltip": false,
            "isCustom": true
        },
        "fechaHasta": {
            "notNull": false,
            "type": "date",
            "required": true,
            "tooltipText": "Ingrese Fecha Hasta",
            "useNumericFormat": false,
            "isPersonalData": false,
            "audited": false,
            "readOnly": false,
            "tooltip": true,
            "isCustom": true
        },
        "listaGym": {
            "type": "enum",
            "required": false,
            "options": [
                "- Seleccionar Gym -",
                "Sportlife",
                "Pasco Club",
                "Pacific Fitness",
                "Centro Xtremo",
                "Brisas del Maipo"
            ],
            "isPersonalData": false,
            "default": "- Seleccionar Gym -",
            "isSorted": true,
            "audited": false,
            "readOnly": false,
            "tooltip": true,
            "isCustom": true,
            "tooltipText": "Ingrese GYM"
        },
        "estadoFormulario": {
            "type": "enum",
            "required": true,
            "options": [
                "- Estado Formulario -",
                "Creado",
                "Inrgesado",
                "Modificado",
                "Eliminado"
            ],
            "isSorted": true,
            "tooltipText": "Ingrese estado",
            "isPersonalData": false,
            "default": "- Estado Formulario -",
            "audited": false,
            "readOnly": false,
            "tooltip": true,
            "isCustom": true
        },
        "observacion": {
            "type": "text",
            "required": false,
            "rows": 30,
            "lengthOfCut": 100,
            "maxLength": 200,
            "seeMoreDisabled": false,
            "tooltipText": "Inrgese Observación con un máximo de: 200 caracteres",
            "default": "Sin Observaciones...",
            "isPersonalData": false,
            "readOnly": false,
            "tooltip": true,
            "isCustom": true
        },
        "documents": {
            "type": "linkMultiple",
            "layoutDetailDisabled": false,
            "layoutMassUpdateDisabled": false,
            "importDisabled": false,
            "noLoad": false,
            "isCustom": true
        },
        "numeroFormulario": {
            "type": "number",
            "len": 36,
            "notNull": false,
            "unique": false,
            "nextNumber": 500002,
            "padLength": 5,
            "prefix": "",
            "tooltipText": "Num",
            "tooltip": true,
            "isCustom": true
        },
        "contact": {
            "type": "link"
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "EntityTeam",
            "layoutRelationshipsDisabled": true
        },
        "meetings": {
            "type": "hasMany",
            "entity": "Meeting",
            "foreign": "parent",
            "layoutRelationshipsDisabled": true
        },
        "calls": {
            "type": "hasMany",
            "entity": "Call",
            "foreign": "parent",
            "layoutRelationshipsDisabled": true
        },
        "tasks": {
            "type": "hasChildren",
            "entity": "Task",
            "foreign": "parent",
            "layoutRelationshipsDisabled": true
        },
        "documents": {
            "type": "hasMany",
            "relationName": "formulariosDocument",
            "foreign": "formularios",
            "entity": "Document",
            "audited": false,
            "isCustom": true
        },
        "contact": {
            "type": "belongsTo",
            "foreign": "formularios",
            "entity": "Contact",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "sortBy": "createdAt",
        "asc": false,
        "textFilterFields": [
            "rut",
            "name"
        ],
        "fullTextSearch": false
    },
    "indexes": {
        "firstName": {
            "columns": [
                "firstName",
                "deleted"
            ]
        },
        "name": {
            "columns": [
                "firstName",
                "lastName"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        }
    }
}